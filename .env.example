
NODE_ENV=development
APP_NAME=NEST_BOILERPLATE
APP_DEFAULT_LANG=en
APP_TIMEZONE="+07:00"
API_VERSION=1
API_VERSION_DEFAULT=1
API_VERSION_KEY=x-api-version
API_PREFIX=version
API_URL=

# SERVER
HOST=0.0.0.0
PORT=4000

# SWAGGER
SWAGGER_PATH=/api/docs
SWAGGER_TITLE=NestAPI
SWAGGER_DESCRIPTION=
SWAGGER_VERSION=1.0
SWAGGER_SCHEME=

# AWS S3
AWS_S3_ACCESS_KEY_ID=
AWS_S3_SECRET_ACCESS_KEY=
S3_BUCKET_NAME=boilerplate-bucket
S3_DEFAULT_LINK_IMAGE_SAVED_PATH=images

# Redis configurations
REDIS_HOST=
REDIS_PASSWORD=
REDIS_PORT=6379
REDIS_TTL=3600
REDIS_PREFIX=boilerplate:
REDIS_URL=redis://(username):(password)@(host):6379

# Rate limiter
RATE_LIMIT_ENABLED=false
RATE_LIMIT_WINDOWMS=2
RATE_LIMIT_MAX=500

HTTP_TIMEOUT=30000
HTTP_MAX_REDIRECTS=5


